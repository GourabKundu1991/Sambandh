{
  "name": "react-native-ssl-public-key-pinning",
  "version": "1.2.5",
  "summary": "Simple and secure SSL public key pinning for React Native. No native configuration needed, set up in <5 minutes.",
  "homepage": "https://github.com/frw/react-native-ssl-public-key-pinning#readme",
  "license": "MIT",
  "authors": "Frederick Widjaja <widjaja.frederick@gmail.com> (https://github.com/frw)",
  "platforms": {
    "ios": "13.4"
  },
  "source": {
    "git": "https://github.com/frw/react-native-ssl-public-key-pinning.git",
    "tag": "1.2.5"
  },
  "source_files": "ios/**/*.{h,m,mm}",
  "compiler_flags": "$(inherited) -DFOLLY_NO_CONFIG -DFOLLY_MOBILE=1 -DFOLLY_USE_LIBCPP=1 -DFOLLY_CFG_NO_COROUTINES=1 -Wno-comma -Wno-shorten-64-to-32",
  "dependencies": {
    "React-Core": [],
    "RCT-Folly": [
      "2022.05.16.00"
    ],
    "glog": [],
    "React-RCTNetwork": [],
    "TrustKit": []
  },
  "pod_target_xcconfig": {
    "HEADER_SEARCH_PATHS": "\"$(PODS_ROOT)/boost\" \"$(PODS_ROOT)/Headers/Private/Yoga\"",
    "CLANG_CXX_LANGUAGE_STANDARD": "c++20"
  }
}
